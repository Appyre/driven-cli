{
  "name": "drive-cli",
  "version": "0.0.1",
  "main": "lib/cli/index.js",
  "description": "A Simple CLI for rapidly building APIs and Mocks based on Waterline.",
  "bin": {
    "driven": "./bin/driven"
  },
  "scripts": {
    "docs": "node bin/generate-docs.js",
    "test": "node tests/runner",
    "test:debug": "node debug tests/runner",
    "test-all": "node tests/runner all",
    "test-all:debug": "node debug tests/runner all",
    "test-all:cover": "istanbul cover tests/runner.js all"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/appyre/driven-cli.git"
  },
  "engines": {
    "node": ">= 0.12.0"
  },
  "keywords": [
    "driven",
    "api",
    "cli"
  ],
  "author": "Chris Thoburn and driven-cli contributors",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/appyre/driven-cli/issues"
  },
  "bundledDependencies": [
    "chalk",
    "configstore",
    "diff",
    "exists-sync",
    "express",
    "findup",
    "fs-extra",
    "git-repo-info",
    "glob",
    "inflection",
    "inquirer",
    "leek",
    "lodash",
    "minimatch",
    "morgan",
    "node-uuid",
    "nopt",
    "npm",
    "quick-temp",
    "readline2",
    "resolve",
    "testem",
    "through",
    "tiny-lr"
  ],
  "devDependencies": {
    "chai": "^3.2.0",
    "exists-sync": "0.0.3",
    "glob": "^5.0.14",
    "minimatch": "^2.0.10",
    "mocha": "^2.2.5",
    "rewire": "^2.3.4",
    "through": "^2.3.8",
    "walk-sync": "^0.1.3"
  },
  "ember-addon": {
    "paths": [
      "./addon/dummy-addon/",
      "./addon/driven/",
      "./node_modules/driven/"
    ]
  },
  "dependencies": {
    "broccoli": "0.16.8",
    "broccoli-concat": "0.0.13",
    "broccoli-funnel": "0.2.8",
    "broccoli-merge-trees": "0.2.3",
    "broccoli-caching-writer": "^2.0.0",
    "chalk": "1.1.0",
    "ember-cli": "ember-cli/ember-cli",
    "exit": "^0.1.2",
    "fs-extra": "0.22.1",
    "lodash": "3.10.0",
    "resolve": "^1.1.6",
    "silent-error": "^1.0.0",
    "symlink-or-copy": "^1.0.1"
  }
}
